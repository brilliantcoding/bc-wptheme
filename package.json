{
  "name": "brilliantcodingwptheme",
  "description": "Build tools for the CSS and the JS for the BrilliantCoding Wordpress Theme.",
  "version": "0.0.1",
  "keywords": [
    "css",
    "sass",
    "web"
  ],
  "homepage": "https://brilliantcoding.com",
  "author": "Matthew Jackowski",
  "watch": {
    "css": {
        "legacyWatch": true,
        "patterns": ["scss"],
        "extensions": "scss"
    }
  },
  "scripts": {
    "bundlesize": "bundlesize",
    "watch_css": "npm-watch css",
    "css": "npm-run-all --parallel css-lint css-compile --sequential css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/bc.scss dist/bc.css",
    "css-lint": "stylelint --config build/.stylelintrc --syntax scss \"scss/*.scss\"",
    "css-lint-vars": "node build/lint-vars.js scss/",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/bc.css\"",
    "css-minify": "cleancss --level 1 --skip-rebase --output brilliantcodingalpha/assets/css/bc.min.css dist/bc.css",
    "js": "npm-run-all js-minify",
    "js-lint": "eslint --config .eslintrc.json js/src/",
    "js-minify": "uglifyjs --mangle --comments \"/^!/\"  --source-map \"includeSources,url=bc.min.js.map\" --output brilliantcodingalpha/assets/js/bc.min.js \"js/src/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twbs/bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/twbs/bootstrap/issues"
  },
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "jquery": "1.9.1 - 3",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.34",
    "@babel/core": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "autoprefixer": "^7.2.1",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.1",
    "bundlesize": "^0.15.3",
    "clean-css-cli": "^4.1.10",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-plugin-compat": "^2.1.0",
    "glob": "^7.1.2",
    "htmllint-cli": "^0.0.6",
    "jsunitsaucelabs": "^1.3.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.2.6",
    "karma-firefox-launcher": "^1.0.1",
    "karma-qunit": "^1.2.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.5",
    "npm-run-all": "^4.1.2",
    "npm-watch": "^0.3.0",
    "popper.js": "^1.13.0",
    "postcss-cli": "^4.1.1",
    "qunitjs": "^2.4.1",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "4.0.0-beta.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "shelljs": "^0.7.8",
    "shx": "^0.2.2",
    "sri-toolbox": "^0.2.0",
    "stylelint": "^8.3.1",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.2.0",
    "uglify-js": "^3.2.1",
    "vnu-jar": "^17.12.7",
    "workbox-build": "^2.1.2"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist/",
    "js/{src,dist}/",
    "scss/"
  ],
  "browserslist": [
    "Chrome >= 45",
    "Firefox ESR",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "bundlesize": [
    {
      "path": "./dist/css/bootstrap-grid.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/css/bootstrap-grid.min.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/css/bootstrap-reboot.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/css/bootstrap-reboot.min.css",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/css/bootstrap.css",
      "maxSize": "25 kB"
    },
    {
      "path": "./dist/css/bootstrap.min.css",
      "maxSize": "20 kB"
    },
    {
      "path": "./dist/js/bootstrap.bundle.js",
      "maxSize": "45 kB"
    },
    {
      "path": "./dist/js/bootstrap.bundle.min.js",
      "maxSize": "25 kB"
    },
    {
      "path": "./dist/js/bootstrap.js",
      "maxSize": "20 kB"
    },
    {
      "path": "./dist/js/bootstrap.min.js",
      "maxSize": "15 kB"
    }
  ]
}
